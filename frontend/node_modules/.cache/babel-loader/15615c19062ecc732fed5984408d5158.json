<b-row\n      v-for=\"row in sponsorGrid\"\n      :key=\"row.id\"\n      class=\"d-flex justify-content-around\"\n    >\n      <b-col\n        v-for=\"sponsor in row\"\n        :key=\"sponsor.id\"\n        sm=\"4\"\n      >\n        <router-link :to=\"'/sponsor?id=' + sponsor.booth_id\">\n          <SponsorCard v-bind:sponsor=\"sponsor\" />\n        </router-link>\n      </b-col>\n    </b-row>\n  </div>\n</template>\n\n<script>\nimport SponsorCard from '@/components/Sponsors/SponsorCard.vue';\n\nexport default {\n  name: 'SponsorSection',\n  components: {\n    SponsorCard,\n  },\n  props: {\n    sponsorList: Array,\n  },\n  data() {\n    return {\n      sponsorGrid: [],\n      numRows: 0,\n    };\n  },\n  async mounted() {\n    this.create_grid();\n  },\n  watch: {\n    // watch prop on change, strangely this is needed for init\n    sponsorList() {\n      this.create_grid();\n    },\n  },\n  methods: {\n    create_grid() {\n      for (let i = 0; i < this.sponsorList.length; i += 1) {\n        if (i % 3 === 0) {\n          this.sponsorGrid.push([]);\n          this.numRows += 1;\n        }\n        this.sponsorGrid[this.numRows - 1].push(this.sponsorList[i]);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.section-banner {\n  max-height: 50px;\n  align-items: center;\n}\n\n.section-title {\n  min-width: 15em;\n  max-width: 15em;\n}\n\n.row {\n  justify-content: center;\n  align-items: center;\n  margin: 1em;\n}\n\n.col-sm {\n  min-width: 15rem;\n  height: 100%;\n  padding-top: 1em;\n  padding-bottom: 1em;\n}\n\n/* horizontal line */\n.hl {\n  border-top: 6px solid var(--color-border);\n  width: 100%;\n  height: 10px;\n}\n</style>\n"],"sourceRoot":"src/components/Sponsors"}]}