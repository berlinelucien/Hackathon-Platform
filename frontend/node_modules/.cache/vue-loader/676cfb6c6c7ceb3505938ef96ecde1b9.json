  components: {\n    Button,\n  },\n  mixins: [GeneralMixin],\n  data() {\n    return {\n      loginButtonClicked: false,\n      userEmail: '',\n      emailNotFound: false,\n      emailInvalid: false,\n    };\n  },\n  methods: {\n    logout() {\n      this.removeUserIdCookie();\n      this.removeUserNameCookie();\n      this.removeUserGroupCookie();\n      this.removeSponsorBoothIdCookie();\n      this.removeCurrentUserCookie();\n      this.$router.push('Login');\n    },\n    goBack() {\n      this.$router.go(-1);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.page-container {\n  width: 100vw;\n  height: 100vh;\n}\n\n.login-container {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  height: 90%;\n}\n\n.content-container {\n  border-radius: 8px;\n  padding: 3rem;\n  background-color: var(--color-foreground);\n  border-radius: var(--border-radius);\n}\n\n@media (max-width: 800px) {\n  .content-container {\n    padding: 1rem;\n  }\n}\n\n@media (min-width: 900px) {\n  .form-control {\n    max-width: 415px;\n  }\n}\n\n.content-container-title {\n  margin-top: 3rem;\n}\n\n.description-text {\n  color: var(--color-muted-text);\n}\n\n.login-footer {\n  margin-bottom: 3rem;\n}\n\n.text-error {\n  color: var(--color-error);\n}\n</style>\n"]}]}