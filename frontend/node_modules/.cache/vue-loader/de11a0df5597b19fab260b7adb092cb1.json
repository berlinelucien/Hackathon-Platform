"data.item.zoom_link\">\n          {{getShorterZoomLink(data.item.zoom_link)}}\n        </ZoomButton>\n      </template>\n    </b-table>\n  </div>\n</template>\n\n<script>\nimport generalMixin from '../mixins/general';\nimport ZoomButton from './ZoomButton';\n\nexport default {\n  name: 'ExpoTable',\n  mixins: [generalMixin],\n  components: {\n    ZoomButton,\n  },\n  props: {\n    items: Array,\n  },\n  data() {\n    return {\n      sortBy: 'time',\n      sortDesc: false,\n      fields: [\n        { key: 'team_name', sortable: true },\n        { key: 'time', sortable: true },\n        { key: 'sponsor_name', sortable: true },\n        { key: 'zoom_link', sortable: false },\n      ],\n      bordered: true,\n      filter: null,\n    };\n  },\n  methods: {\n    getShorterZoomLink(link) {\n      return link.split('?')[0];\n    },\n  },\n};\n</script>\n"]}]}